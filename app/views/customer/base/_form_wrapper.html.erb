<%= simple_form_for [:customer, @object], wrapper: 'horizontal_form' do |f| %>
  <%= render 'form', f: f %>

  <div class='actions'>
    <%= f.submit t('core.links.create'), class: 'btn btn-success' %>
    <%= link_to t('core.links.back'), [:customer, @object.class], class: 'btn btn-default' %>
  </div>
<% end %>
